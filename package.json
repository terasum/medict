{
  "name": "medict",
  "productName": "medict",
  "version": "2.0.1",
  "description": "Medict an elegant dictionary app",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts .",
    "test": "jest"
  },
  "keywords": [],
  "author": {
    "name": "terasum",
    "email": "terasum@163.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "medict"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/renderer/renderer.html",
                  "js": "./src/renderer/renderer.ts",
                  "name": "main_window"
                },
                {
                  "html": "./src/renderer/subwin/dict-settings.window.html",
                  "js": "./src/renderer/subwin/dict-settings.window.ts",
                  "name": "dict_settings_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.56",
    "@electron-forge/maker-deb": "^6.0.0-beta.56",
    "@electron-forge/maker-rpm": "^6.0.0-beta.56",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.56",
    "@electron-forge/maker-zip": "^6.0.0-beta.56",
    "@electron-forge/plugin-webpack": "6.0.0-beta.56",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/bootstrap": "^5.0.17",
    "@types/chai": "^4.2.19",
    "@types/expect": "^24.3.0",
    "@types/jest": "^26.0.23",
    "@types/koa": "^2.13.3",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-pino-logger": "^3.0.0",
    "@types/koa-static": "^4.0.1",
    "@types/lodash": "^4.14.170",
    "@types/tmp": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.4",
    "chai": "^4.3.4",
    "css-loader": "^5.0.0",
    "electron": "13.1.2",
    "electron-squirrel-startup": "^1.0.0",
    "eslint": "^7.29.0",
    "eslint-plugin-import": "^2.20.0",
    "fork-ts-checker-webpack-plugin": "^6.2.12",
    "jest": "^27.0.6",
    "node-loader": "^2.0.0",
    "postcss": "^8.1.0",
    "postcss-loader": "^6.1.0",
    "postcss-pxtorem": "^6.0.0",
    "sass": "^1.35.2",
    "sass-loader": "^12.1.0",
    "style-loader": "^2.0.0",
    "ts-jest": "^27.0.4",
    "ts-loader": "^9.2.2",
    "typescript": "^4.3.4",
    "vue-loader": "^15.9.7",
    "vue-style-loader": "^4.1.3",
    "webpack": "^5.0.0"
  },
  "dependencies": {
    "@popperjs/core": "^2.9.2",
    "@shuaninfo/random": "^5.1.1",
    "autoprefixer": "^10.2.6",
    "axios": "^0.21.1",
    "bootstrap": "^5.0.2",
    "bootstrap-icons": "^1.5.0",
    "bootstrap-vue": "^2.21.2",
    "cheerio": "^1.0.0-rc.10",
    "dotenv": "^10.0.0",
    "electron-log": "^4.3.5",
    "get-port": "^5.1.1",
    "jquery": "1.9.1 - 3",
    "js-mdict": "4.0.15",
    "koa": "^2.13.1",
    "koa-error": "^3.2.0",
    "koa-logger": "^3.2.1",
    "koa-pino-logger": "^3.0.0",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.21",
    "lowdb": "^2.1.0",
    "tmp": "^0.2.1",
    "vue": "^2.6.14",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.5.2",
    "vue-template-compiler": "^2.6.14",
    "vuex": "^3.6.2"
  }
}
